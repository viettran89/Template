{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script",
    "ecmaFeatures": {}
  },
  "rules": {
    "no-undef": 0,
    "camelcase": 2,
    "quotes": [2, "single"],
    "indent": [2, 2],
    "semi": [2, "always"],
    "semi-spacing": [1, {"before": false, "after": true}],
    "space-unary-ops": 2,
    "space-infix-ops": [2, {"int32Hint": false}],
    "space-before-function-paren": 1,
    "switch-colon-spacing": [1, {"before": false, "after": true}]
  },
  "env": {}
}
